{
    "builds": [{
        "src": "SED/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }, {
        "src": "build_file.sh",
        "use": "@vercel/static-build",
        "config": { "distDir": "staticfiles" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "SED/wsgi.py"
        }
    ]
}